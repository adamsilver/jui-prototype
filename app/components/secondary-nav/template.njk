<nav class="jui-secondary-nav" aria-label="{{params.label}}">
  <ul class="jui-secondary-nav__list">
    {% for item in params.items %}
      <li class="jui-secondary-nav__item">
        <a class="jui-secondary-nav__link" {{ 'aria-current=page' if item.selected }} href="{{item.href}}">{{item.text}}</a>
      </li>
    {% endfor %}
  </ul>
</nav>